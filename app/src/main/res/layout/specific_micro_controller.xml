<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    >





        <ImageView
            android:id="@+id/DevicesImage_Lay_specific_micro_controller"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/air_conditioner"
            android:scaleType="centerCrop"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="50dp"

            />

        <ImageView
            android:id="@+id/ShieldsImage_Lay_specific_micro_controller"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/air_conditioner"
            android:scaleType="centerCrop"
            android:layout_marginLeft="50dp"
            android:layout_toRightOf="@+id/DevicesImage_Lay_specific_micro_controller"
            android:layout_marginTop="50dp"

            />

    </RelativeLayout>









